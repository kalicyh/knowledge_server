{
  "name": "knowledge_backend_vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "setup": "yarn install && pip3 install poetry && poetry install",
    "fastapi-dev": "poetry run uvicorn api.main:app --reload",
    "vue-dev": "vue-cli-service serve",
    "dev": "concurrently --kill-others-on-fail \"yarn run fastapi-dev\" \"yarn run vue-dev\"",
    "test": "yarn run build && yarn run fastapi-dev",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:docker": "docker build -t knowledge_backend:$(node -p \"require('./package.json').version\") .",
    "run:docker": "docker run -p 8000:8000 knowledge_backend:$(node -p \"require('./package.json').version\")",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "core-js": "^3.8.3",
    "vue": "^3.2.13",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
