{
  "name": "knowledge_server_vue",
  "version": "1.5.0",
  "private": true,
  "scripts": {
    "setup": "yarn install && pip3 install poetry && poetry install",
    "fastapi-dev": "poetry run uvicorn api.main:app --reload",
    "vite-dev": "vite",
    "dev": "concurrently --kill-others-on-fail \"yarn run fastapi-dev\" \"yarn run vite-dev\"",
    "test": "yarn run build && yarn run fastapi-dev",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "build:docker": "docker build -t knowledge_server:$(node -p \"require('./package.json').version\") .",
    "run:docker": "docker run -p 8000:8000 knowledge_server:$(node -p \"require('./package.json').version\")"
  },
  "dependencies": {
    "@braks/vue-flow": "0.4.41",
    "@tiptap/core": "^2.5.9",
    "@tiptap/pm": "^2.5.9",
    "@tiptap/starter-kit": "2.5.9",
    "@tiptap/vue-3": "2.5.9",
    "@types/lodash": "^4.17.7",
    "@vue/compiler-sfc": "^3.2.6",
    "apexcharts": "3.52.0",
    "axios": "^1.7.3",
    "axios-retry": "^4.5.0",
    "date-fns": "^3.6.0",
    "lodash": "^4.17.21",
    "maska": "^3.0.0",
    "vite-plugin-vuetify": "2.0.4",
    "vue": "3.4.37",
    "vue-router": "4.4.3",
    "vue-tabler-icons": "2.21.0",
    "vue3-apexcharts": "1.5.3",
    "vue3-perfect-scrollbar": "2.0.0",
    "vuetify": "3.6.14",
    "webpack": "^5.0.0",
    "xlsx": "^0.18.5",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@mdi/font": "^7.4.47",
    "@tsconfig/node18": "^18.2.4",
    "@types/chance": "^1.1.3",
    "@types/node": "18",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vue/tsconfig": "^0.5.1",
    "naive-ui": "^2.39.0",
    "sass": "^1.49.9",
    "sass-loader": "^16.0.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.0",
    "vue-cli-plugin-vuetify": "2.5.8",
    "vue-tsc": "2.0.29",
    "vuetify-loader": "^2.0.0-alpha.9"
  }
}
